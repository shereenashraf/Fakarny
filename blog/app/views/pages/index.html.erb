<h1>Listing Pages</h1>
<%= link_to 'New Page', new_page_path %>
<table>
  <tr>
   <th>Page Name</th>
   <th colspan="2"></th>
  </tr>
 
  <% @pages.each do |page| %>
    <tr>
     
      <td><%= page.providername %></td>
      <td><%= link_to 'Show', page_path(page) %></td>
       <td><%= link_to 'Edit', edit_page_path(page) %></td>
    </tr>
  <% end %>
</table>
<%= link_to 'Back', pages_path %>